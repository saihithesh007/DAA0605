#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char* countAndSay(int n) {
    if (n == 1) return "1";
    
    char* result = (char*)malloc(100 * sizeof(char));
    strcpy(result, "1");
    
    for (int i = 2; i <= n; i++) {
        char* temp = (char*)malloc(100 * sizeof(char));
        int count = 1, index = 0;
        
        for (int j = 0; result[j] != '\0'; j++) {
            if (result[j] == result[j + 1]) {
                count++;
            } else {
                index += sprintf(temp + index, "%d%c", count, result[j]);
                count = 1;
            }
        }
        temp[index] = '\0';
        strcpy(result, temp);
        free(temp);
    }
    
    return result;
}

int main() {
    int n = 4; // Change this value to test with different inputs
    char* result = countAndSay(n);
    printf("Output: %s\n", result);
    return 0;
}
