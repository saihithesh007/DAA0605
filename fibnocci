def fibonacci(n):
    if n<=0:
      return 0
    elif n==1:
        return 1
    else:
       return fibonacci(n-1)+fibonacci(n-2)


def print_fibnocci(term):
    "printing the fibnocci series"
    for i in range(term):
        print(fibonacci(i),end=' ')
    print()

terms=int(input("enter the number of terms"))
print_fibnocci(terms)
