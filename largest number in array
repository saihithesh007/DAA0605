def find_largest_element(arr):
    if not arr:
        return None
    largest = arr[0]
    for i in arr:
        if i > largest:
            largest = i
    return largest

array = [3, 1, 4, 1, 5, 9, 2, 6, 5]
largest_element = find_largest_element(array)
print("The largest element is:", largest_element)
